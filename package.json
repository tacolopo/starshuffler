{
  "name": "juno-privacy-mixer",
  "version": "1.0.0",
  "description": "Privacy mixer for Juno blockchain",
  "dependencies": {
    "@cosmjs/cosmwasm-stargate": "^0.33.0",
    "@cosmjs/proto-signing": "^0.33.0",
    "@cosmjs/stargate": "^0.33.0",
    "circomlib": "^2.0.0",
    "dotenv": "^16.0.3",
    "express-rate-limit": "^7.5.0",
    "ioredis": "^5.5.0",
    "rate-limit-redis": "^4.2.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "deploy-contract": "ts-node scripts/deploy-contract.ts",
    "setup-circuit": "cd circuit && npm install && npm run setup",
    "convert-key": "cargo run --bin convert_verification_key",
    "build-contract": "docker run --rm -e DOCKER_OPTIMIZER=1 -v \"$(pwd)\":/code -v \"$(pwd)/src/verification_key:/code/src/verification_key\" --mount type=volume,source=\"$(basename \"$(pwd)\")_cache\",target=/target --mount type=volume,source=registry_cache,target=/usr/local/cargo/registry cosmwasm/optimizer:0.16.1",
    "deploy": "npm run setup-circuit && npm run convert-key && npm run build-contract && npm run deploy-contract",
    "setup": "ts-node circuit/setup.ts",
    "fix-key": "ts-node scripts/fix-verification-key.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/express-rate-limit": "^5.1.3"
  }
}
